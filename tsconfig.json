{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "*": [ "node_modules/*" ],
      "@App/*": [ "App/*" ],
      "@Database/*": [ "Database/*" ],
      "@Models/*": [ "Models/*" ]
    }
  },
  "include": [
      "src/**/*"
  ]
}
